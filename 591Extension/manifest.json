{
  "background": {
    "persistent": false,
    "scripts": ["jquery.min.js","background.js"]
  },
  "description": "CSE 591 Extension",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "notifications",
    "gcm"

 ],
 "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["jquery.min.js","content.js"]
  }],
  "name": "CSE 591 Extension",
  "version": "0.1",
  "manifest_version": 2

}
